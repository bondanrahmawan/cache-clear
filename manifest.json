{
	"manifest_version": 3,
	"name": "One-Click Cache Cleaner",
	"version": "1.0",
	"description": "Clear cache with one button click.",
	"permissions": ["browsingData"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "Clear Cache"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
